<style>
.wv-custom-header {
  display: flex;
  flex-direction: column;
  height: 100%;
  justify-content: space-between;
  max-height: 100vh;
  position: sticky;
  top: 0;
  padding: 10px;
  width: 200px;
}
.wv-custom-header .header-list {
  display: flex;
  flex-direction: column;
  gap: 10px;
}
.wv-custom-header .header-list > a {
  cursor: pointer; 
  background: transparent;
  border: none;
  width: 100%;
  text-align: left;
  font-weight: bolder;
  display: flex;
  gap: 20px;
  align-items: center;
  text-decoration: none;
  justify-content: flex-start;
}
.wv-custom-header .header-list > a .image-container {
  display: flex;
}
.wv-custom-header .header-list > a svg {
  height: 35px;
  width: 35px;
}
.wv-custom-header .header-list > a span {
  font-size: var(--paragraph-font-size);
  font-family: var(--main-heading-font-family);
}
.wv-custom-header .cart-header-container .image-container {
  position: relative;
}
.wv-custom-header .cart-header-container .image-container .cart-qty-container {
  padding: 1px 6px;
  border-radius: 50%;
  top: -7px;
  right: -10px;
  position: absolute;
  background: var(--accent-color);
  color: var(--off-white);
  width: 23px;
  height: 25px;
  display: flex;
  justify-content: center;
  transition: 1s ease-in-out;
}
#header-cart-count {
  font-size: 14px;
  transition: 0.3s ease-in-out;
  color: var(--off-black);
  transform: scale(1.3);
}
@media screen and (max-width: 450px) {
  .wv-custom-header .header-list > a {
    justify-content: center;
  }
  .wv-custom-header .header-list > a svg {
    height: 35px;
    width: 35px;
  }
}

/* ////////////////////////////////////////////////////////////// */
/* Header mobile view can be found at wv-custom-stylesheet.liquid */
/* ////////////////////////////////////////////////////////////// */

/* Added to cart popup */
#added-to-cart-popup {
  position: absolute;
  top: -180px;
  right: 10px;
  display: flex;
  border: 1px solid;
  border-radius: 5px;
  padding: 5px;
  align-content: center;
  justify-content: center;
  gap: 5px;
  max-width: 450px;
  transition: 0.3s ease-in-out;
  background: var(--pure-white);
}
#added-to-cart-popup img {
  height: 50px;
  width: 50px;
}
#added-to-cart-popup div p,
#added-to-cart-popup div h6 {
  margin: 0;
  padding: 0;
}
#added-to-cart-popup > div {
  display: flex;
  flex-direction: column;
  gap: 0px;
}
#added-to-cart-popup div p {
  font-size: 11px;
}
#added-to-cart-popup div h6 {
  font-size: 14px;
}
#added-to-cart-popup svg {
  height: 15px;
  width: 25px;
  align-self: baseline;
}
@media screen and (max-width: 490px) {
  #added-to-cart-popup {
    max-width: 350px;
  }
}
@media screen and (max-width: 400px) {
  #added-to-cart-popup {
    max-width: 300px;
  }
}

/* Creator profile twinkle animation */
@keyframes twinkle {
  0%, 100% {
    fill: var(--off-black);
  }
  50% {
    fill: var(--accent-color);
  }
}
.creator-profile-link .image-container svg > g > circle:nth-of-type(1) {
  animation: twinkle 1.5s ease-in-out;
  animation-delay: 0s;
}

.creator-profile-link .image-container svg > g > circle:nth-of-type(2) {
  animation: twinkle 1.5s ease-in-out;
  animation-delay: 0.4s;
}

.creator-profile-link .image-container svg > g > path:nth-of-type(2) {
  animation: twinkle 1.5s ease-in-out;
  animation-delay: 0.8s;
}

.creator-profile-link .image-container svg > g > path:nth-of-type(3) {
  animation: twinkle 1.5s ease-in-out;
  animation-delay: 1.2s;
}

.creator-profile-link .image-container svg > g > path:nth-of-type(4) {
  animation: twinkle 1.5s ease-in-out;
  animation-delay: 1.6s;
}
</style>

{% assign is_creator = false %}
{% if customer and customer.metafields.custom.creator_page %}
  {% assign is_creator = true %}
{% endif %}

{% assign outside_homepage = false %}
{% if template != 'index' %}
  {% assign outside_homepage = true %}
{% endif %}

{% assign creator_template = false %}
{% if template contains 'int-' or template contains 'creator' or template == 'product' or template == 'customers/account' %}
  {% assign creator_template = true %}
{% endif %}

{% assign internal_page = false %}
{% if template contains 'page' and template != 'product' %}
  {% unless template contains 'pub-' %}
    {% assign internal_page = true %}
  {% endunless %}
{% else %}
  {% assign internal_page = true %}
{% endif %}

{% assign show_creator_header = false %}
{% if is_creator and outside_homepage and creator_template and internal_page %}
  {% assign show_creator_header = true %}
{% endif %}

{% assign creator_sold_product = false %}
{% if template == 'product' %}
  {% for metafieldProduct in customer.metafields.custom.products_sold_by_creator.value %}
    {% if metafieldProduct.id == product.id %}
      {% assign creator_sold_product = true %}
      {% break %}
    {% endif %}
  {% endfor %}
{% else %}
  {% assign creator_sold_product = true %}
{% endif %}

{% assign show_creator_header_bool = false %}
{% if show_creator_header and creator_sold_product %}
  {% assign show_creator_header_bool = true %}
{% endif %}

