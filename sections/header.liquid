<script>

// Global update cart function
async function updateCartCount() {
  try {
    document.querySelector('.wv-custom-header .cart-header-container .image-container .cart-qty-container span').style.color = 'var(--off-black)';
    document.querySelector('.wv-custom-header .cart-header-container .image-container .cart-qty-container span').style.opacity = '0';
    
    const res = await fetch('/cart.js');
    const cart = await res.json();

    const count = cart.item_count;
    // console.log('Current cart count: ', count);
    document.getElementById('header-cart-count').textContent = count;

    document.querySelector('.wv-custom-header .cart-header-container .image-container .cart-qty-container span').style.color = 'var(--off-black)';
    document.querySelector('.wv-custom-header .cart-header-container .image-container .cart-qty-container span').style.opacity = '1';    
  } catch (err) {
    console.error('Error updating cart count:', err);
  };
};
{% comment %} document.addEventListener('DOMContentLoaded', () => {
  setTimeout(() => {
    updateCartCount();
  }, 100);
}); {% endcomment %}

// global func to display added to cart popup
function addedItemToCart(cartItem) {
  console.log('Going to display the following:', cartItem.product_title, cartItem.featured_image.url );

  if (!cartItem.product_title || !cartItem.featured_image.url) {
    console.log('Added to cart popup missing values: ', cartItem.product_title, cartItem.featured_image.url);
    return;
  };

  const container = document.querySelector('#added-to-cart-popup');
  const title = document.querySelector('#added-to-cart-popup  div h6');
  const imgSrc = document.querySelector('#added-to-cart-popup > img');

  title.textContent = cartItem.product_title;
  imgSrc.src = cartItem.featured_image.url;
  imgSrc.alt = cartItem.product_title;

  // give the img a bit of time to load
  setTimeout(() => {
    container.style.top = '10px';
  }, 500);

  setTimeout(() => {
    container.style.top = '-182px';
  }, 4500);
};
</script>

